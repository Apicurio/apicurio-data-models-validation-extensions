{
	"compilerOptions": {
	  "declaration": true,
	  "stripInternal": true,
	  "experimentalDecorators": true,
	  "noImplicitAny": false,
	  "noFallthroughCasesInSwitch": true,
	  "suppressImplicitAnyIndexErrors": true,
	  "preserveConstEnums": true,
	  "removeComments": false,
	  "module": "commonjs",
	  "moduleResolution": "node",
	  "sourceMap": true,
	  "inlineSources": true,
	  "target": "es5",
	  "lib": ["es2015", "dom"],
	  "skipLibCheck": true
	},
	"exclude": [
	  "node_modules",
	  "tests"
	]
  }